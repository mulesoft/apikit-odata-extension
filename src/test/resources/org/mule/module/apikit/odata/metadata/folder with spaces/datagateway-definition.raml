#%RAML 1.0
title: Auto-generated RAML for DataGateway
version: 0.1
mediaType:  application/json
uses:
  odata: ../odataLibrary.raml
  model: ../odata.raml


resourceTypes:
  collection:
    is: [odata.orderby, odata.top, odata.skip, odata.filter, odata.expand, odata.format, odata.select, odata.inlinecount]
    get:
      description: List of <<model | !pluralize>>
      responses:
        200:
          body:
            application/json:
              type: <<model>>[]
    post:
      description: Create a new <<model>>
      responses:
        200:
          body:
            application/json:
              type: <<model>>
  member:
    is: [odata.filter, odata.expand, odata.format, odata.select]
    get:
      description: Read <<model>>
      responses:
        200:
          body:
            application/json:
              type: <<model>>
    delete:
      description: Delete <<model>>
      responses:
        200:
          body:
            application/json:
              type: <<model>>
    put:
      description: Update <<model>>
      body:
        application/json:
          type: <<model>>
      responses:
        204:
          body:
            application/json:
              type: <<model>>

/gateways:
    displayName: gateways
    type: { collection: { model: model.gateways } }
    /gatewaysId:
        displayName: gateways Id
        type: { member: { model: model.gateways } }
/users:
    displayName: users
    type: { collection: { model: model.users } }
    /usersId:
        type: { member: { model: model.users } }
